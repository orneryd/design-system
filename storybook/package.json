{
  "name": "storybook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "SKIP_PREFLIGHT_CHECK=true && react-scripts start",
    "build": "SKIP_PREFLIGHT_CHECK=true && react-scripts build",
    "test": "SKIP_PREFLIGHT_CHECK=true && react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "SKIP_PREFLIGHT_CHECK=true && build-storybook -c .storybook -o build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-docs": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-storyshots": "^5.2.6",
    "@storybook/addon-storysource": "^5.3.13",
    "@storybook/addons": "^5.2.6",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.2.6",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.7.1",
    "jest": "^25.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.0",
    "react-test-renderer": "^16.12.0",
    "require-context.macro": "^1.2.2",
    "styled-components": "^5.0.1",
    "styled-theming": "^2.2.0",
    "typescript": "^3.7.5"
  }
}
